   "source": [
    "def n_gram(sentence, n):\n",
    "    lst = []\n",
    "    for i in range(len(sentence) - 1):\n",
    "        lst.append(sentence[i:i + n])\n",
    "    return lst\n",
    "\n",
    "\n",
    "sentence = 'I am an NLPer'\n",
    "\n",
    "# 文字bi-gram\n",
    "print(n_gram(sentence.split(), 2))\n",
    "\n",
    "# 単語bi-gram\n",
    "print(n_gram(sentence, 2))\n"
   ]
