    "def cipher(message):\n",
    "    result = ''\n",
    "    for c in message:\n",
    "        result += chr(219 - ord(c))\n",
    "    return result\n",
    "    \n",
    "    \n",
    "message = 'password'\n",
    "\n",
    "# 暗号化\n",
    "code = cipher(message)\n",
    "print(code)\n",
    "\n",
    "# 復号化\n",
    "decrypted_code = cipher(code)\n",
    "print(decrypted_code)"
